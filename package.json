{
  "name": "lab2pdf",
  "version": "1.0.6",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "test": "mocha --require @babel/register --timeout 5000 tests/template.test.js",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.ripflame.lab2pdf",
    "productName": "Lab2PDF",
    "publish": {
      "provider": "github",
      "owner": "ripflame",
      "repo": "Lab2PDF"
    },
    "directories": {
      "output": "dist"
    },
    "extraResources": [
      {
        "from": "assets",
        "to": "assets"
      }
    ],
    "mac": {
      "icon": "assets/icon.icns"
    },
    "win": {
      "icon": "assets/icon.ico"
    },
    "asar": true,
    "asarUnpack": [
      "**/node_modules/sharp/**/*",
      "**/node_modules/@img/**/*"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/register": "^7.21.0",
    "cross-env": "^7.0.3",
    "electron": "^34.1.1",
    "electron-builder": "^23.6.0",
    "electron-reload": "^2.0.0-alpha.1",
    "mocha": "^10.2.0"
  },
  "dependencies": {
    "electron-updater": "^5.3.0",
    "puppeteer": "^24.2.0",
    "sharp": "^0.33.5"
  }
}
